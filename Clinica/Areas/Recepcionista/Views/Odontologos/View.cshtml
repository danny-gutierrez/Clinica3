@model Clinica.Models.Odontologo
<!DOCTYPE html>
<html lang="en">

<head>


    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>SB Admin 2 - Register</title>

    <!-- Custom fonts for this template-->
    <link href="vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">

    <!-- Custom styles for this template-->
    <link href="css/sb-admin-2.min.css" rel="stylesheet">

</head>



<body class="bg-gradient-primary">
    <center>


        <div class="container">

            <div class="">
                <div class="">
                    <!-- Nested Row within Card Body -->


                    <div class="row">
                        <div class="col-lg-5 d-none  bg-light"> </div>
                        <div class="col-lg-10">
                            <div class="p-5">
                                <div class="text-center">
                                    <div class="card-body">

                                        <div class="card shadow mb-4">
                                            <div class="card-header py-3">

                                                <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                                                    <h1 class="h4 text-gray-900 mb-4">Ver Odontologo</h1>
                                                    <hr />
                                            </div>


                                            <form method="POST" action="" @Url.Action("Index", "Odontologos") " class="user">
                                                <div class="form-group row">
                                                    <div class="col-sm-6 mb-3 mb-sm-0">
                                                        <label>Nombre </label>
                                                        <input type="text" class="form-control form-control-user" placeholder="Ingrese Nombre Odontologo"
                                                               name="Nombres" value="@Model.Nombres" disabled>
                                                   
                                                    </div>


                                                    <div class="col-sm-6">

                                                        <label>Apellido</label>
                                                        <input type="text" class="form-control form-control-user" placeholder="Ingrese Apellido Odontologo"
                                                               name="Apellidos" value="@Model.Apellidos" disabled>
                                                       
                                                    </div>

                                                </div>

                                                <div class="form-group row">
                                                    <div class="col-sm-6 mb-3 mb-sm-0">

                                                        <label>Telefono</label>
                                                        <input type="text" class="form-control form-control-user" placeholder="Ingrese Telefono"
                                                               name="Telefonos" value="@Model.Telefonos" disabled>
                                                       
                                                    </div>

                                                    <div class="col-sm-6">

                                                        <label>DIreccion</label>
                                                        <input type="text" class="form-control form-control-user" placeholder="Ingrese Direccion"
                                                               name="Direcciones" value="@Model.Direcciones" disabled>
                                                       
                                                    </div>



                                                </div>
                                                <div class="form-group row">
                                                    <div class="col-sm-6 mb-3 mb-sm-0">

                                                        <label>Correo</label>
                                                        <input type="email" class="form-control form-control-user" placeholder="Ingrese Correo"
                                                               name="Correos" value="@Model.Correos" disabled>
                                                       

                                                    </div>

                                                    <div class="col-sm-6 mb-3 mb-sm-0">

                                                        <label>Rut</label>
                                                        <input type="text" class="form-control form-control-user" placeholder="Ingrese Rut"
                                                               name="Rut" value="@Model.Rut" disabled>
                                                       

                                                    </div>
                                                    <div class="col-sm-6 mb-3 mb-sm-0">

                                                        <br />

                                                        <label>Fecha de Ingreso</label>
                                                        <input type="text" class="form-control form-control-user" placeholder="Fecha de Ingreso"
                                                               name="FechaIngreso" value="@Model.FechaIngreso" disabled>
                                                     

                                                    </div>
                                                    <div class="col-sm-6 mb-3 mb-sm-0">

                                                        <br />

                                                        <label>Edad</label>
                                                        <input type="text" class="form-control form-control-user" placeholder="Edad"
                                                               name="Edad" value="@Model.Edad" disabled>
                                                      

                                                    </div>


                                                </div>
                                                <div class="form-group">
                                                    <center>

                                                        <label for="sel1" style="color:#212F3D">Especialidad </label>
                                                    </center>
                                                    <select class="form-control" name="Especialidad" value="@Model.Especialidad" disabled>
                                                        <option>Endodoncia</option>
                                                        <option>Cirugia</option>
                                                        <option>Blanqueamiento</option>
                                                        <option>Otro</option>
                                                    </select>



                                                    <div class="form-group row">


                                                    </div>
                                                    <hr />
                                                    <!--  <a href="" class="btn btn-primary btn-user btn-block"> -->
                                                    <center>
                                                        <a href="@Url.Action("Index", "Odontologos")"><input type="button" value="Volver" class="btn btn-primary"></a>
                                                    </center>
                                                    </a>




                                            </form>

                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
                </table>


            </div>
        </div>
        <!-- Bootstrap core JavaScript-->
        <script src="vendor/jquery/jquery.min.js"></script>
        <script src="vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

        <!-- Core plugin JavaScript-->
        <script src="vendor/jquery-easing/jquery.easing.min.js"></script>

        <!-- Custom scripts for all pages-->
        <script src="js/sb-admin-2.min.js"></script>
    </center>
</body>

</html>